{
    "name": "Scala",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "scalameta.metals",
                "scala-lang.scala",
                "-vscjava.vscode-java-pack"
            ]
        }
    },
    "postCreateCommand": "sbt update",
    "forwardPorts": [
        8080
    ],
    "remoteUser": "vscode"
}
